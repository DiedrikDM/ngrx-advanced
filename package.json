{
  "name": "ngxr-advanced",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run server\" \"ng serve -o\"",
    "start:ssr": "concurrently \"npm run server\" \"npm run serve:ssr\"",
    "server": "json-server db.json --watch",
    "start:prod": "npx serve -s ./dist/ngxr-advanced",
    "start:prod:db": "concurrently \"npm run server\" \"npm run start:prod\"",

    "http:server":"npx http-server -p 8080 ./dist/ngxr-advanced",
    "http:server:db":"concurrently \"npm run server\" \"npm run http:server\"",
    "generate:universal": "ng generate",
    "build": "ng build ngxr-advanced --prod",
    "dev:ssr": "ng run ngxr-advanced:serve-ssr",
    "serve:ssr": "node dist/ngxr-advanced/server/main.js",
    "build:ssr": "ng build --prod && ng run ngxr-advanced:server:production",
    "prerender:dynamic": "concurrently \"npm run server\" \"npm run prerender\"",
    "prerender": "ng run ngxr-advanced:prerender",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.9",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "~10.0.9",
    "@angular/compiler": "~10.0.9",
    "@angular/core": "~10.0.9",
    "@angular/forms": "~10.0.9",
    "@angular/material": "^10.1.3",
    "@angular/platform-browser": "~10.0.9",
    "@angular/platform-browser-dynamic": "~10.0.9",
    "@angular/platform-server": "^10.0.9",
    "@angular/router": "~10.0.9",
    "@angular/service-worker": "~10.0.9",
    "@ngrx/effects": "^10.0.0",
    "@ngrx/entity": "^10.0.0",
    "@ngrx/router-store": "^10.0.0",
    "@ngrx/store": "^10.0.0",
    "@ngrx/store-devtools": "^10.0.0",
    "@nguniversal/express-engine": "^10.0.2",
    "bootstrap": "^4.5.2",
    "express": "^4.17.1",
    "localforage": "^1.9.0",
    "localstorage-polyfill": "^1.0.1",
    "ngx-quicklink": "^0.2.3",
    "rxjs": "~6.6.2",
    "tslib": "^2.0.1",
    "uuid": "^8.3.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.6",
    "@angular/cli": "~10.0.6",
    "@angular/compiler-cli": "~10.0.9",
    "@angular/language-service": "~10.0.9",
    "@nguniversal/builders": "^10.0.2",
    "@types/express": "^4.17.7",
    "@types/jasmine": "^3.5.12",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.54",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.3.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "json-server": "^0.16.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "^6.1.3",
    "typescript": "~3.9.5"
  }
}
